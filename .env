# Docker registry
ENV_REGISTRY_HOST=registry.dh.unimaas.nl
ENV_TAG=latest
ENV_BRANCH=master

# iRODS and iRES
ENV_IRODS_VERSION=4.2.6
ENV_IRODS_EXT_CLANG_VERSION=3.8-0
ENV_IRODS_EXT_CLANG_RUNTIME_VERSION=3.8-0
